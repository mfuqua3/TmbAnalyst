@page
@model Index
@{
    ViewData["Title"] = "TMB Import";
    ViewData["Script"] = "Import";
}
<partial name="_AlertPartial"/>
<form method="post">
    <div class="form-group">
        <label asp-for="RawImportString"></label>
        <textarea asp-for="RawImportString" 
        class="form-control form-text" rows="18"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">
        Import
    </button>
</form>